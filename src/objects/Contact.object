9, &quot;Delinquent&quot;,
8, &quot;Overdue&quot;,
7, &quot;Due&quot;,
6, &quot;On Payment Plan&quot;,
5, &quot;Paid&quot;,
4, &quot;Paid&quot;,
3, &quot;Paid&quot;,
