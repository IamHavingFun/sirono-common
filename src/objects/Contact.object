        <fullName>Add_to_Payment_Plan__c</fullName>
        <externalId>false</externalId>
        <formula>AND(Payment_Plan_Balance__c &gt; 0, Payment_Plan_Balance__c &lt;&gt; Invoiced_Balance__c)</formula>
