    <fields>
        <fullName>Address__c</fullName>
        <externalId>false</externalId>
        <formula>MailingStreet + BR() + MailingCity + &quot;, &quot; + MailingState + &quot; &quot; + MailingPostalCode + &quot; &quot; + MailingCountry</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Address</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Age__c</fullName>
        <description>Contacts&apos;s age in years</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Birthdate), NULL, YEAR(Today()) - YEAR(Birthdate) - IF(DATE(2000, MONTH(Birthdate), DAY(Birthdate)) &gt; DATE(2000, MONTH(Today()), DAY(Today())), 1, 0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
