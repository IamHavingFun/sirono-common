    <fields>
        <fullName>Next_Payment_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Skip_Payment_Until__c),
IF(Plan_Type__c = &quot;Automatic&quot;,
  IF(Execute_on_Day__c &gt;= DAY(Today()),
    Today() + MIN(DATE(YEAR(Today()), MONTH(Today()), 1) + Execute_on_Day__c - Today(), IF(MONTH(Today()) = 12, DATE(YEAR(Today()) + 1, 1, 1), DATE(YEAR(Today()), MONTH(Today()) + 1, 1)) - Today()),
    IF(MONTH(Today()) = 12,
      DATE(YEAR(Today()) + 1, 1, 1) + Execute_on_Day__c,
      Today() + MIN(DATE(YEAR(Today()), MONTH(Today()) + 1, 1) + Execute_on_Day__c - Today(), DATE(YEAR(Today()), MONTH(Today()) + 2, 1) - Today()))) - 1,
  IF(DAY(Today()) &gt; Execute_on_Day__c + 21,
    IF(MONTH(Today()) = 12, DATE(YEAR(Today()) + 1, 1, 1), DATE(YEAR(Today()), MONTH(Today()) + 1, 1)),
    IF(MONTH(Today()) = 1,
      IF(DATE(YEAR(Today()) - 1, 12, Execute_on_Day__c) + 21 &gt; Today(),
        DATE(YEAR(Today()) - 1, 12, 1),
        DATE(YEAR(Today()), MONTH(Today()), 1)),
      IF(DATE(YEAR(Today()), MONTH(Today()) - 1, Execute_on_Day__c) + 21 &gt; Today(),
        DATE(YEAR(Today()), MONTH(Today()) - 1, 1),
        DATE(YEAR(Today()), MONTH(Today()), 1)))) + Execute_on_Day__c + 20),
  Skip_Payment_Until__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Payment Date_old</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
