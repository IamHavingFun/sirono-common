    <fields>
        <fullName>Has_Invoiced_Balance__c</fullName>
        <description>This function returns TRUE if this Charge Group balance is viewed as &quot;Invoiced.&quot;</description>
        <externalId>false</externalId>
        <formula>AND(Account_Status__c = &quot;Billed&quot;,
  IF (CASE(Account_Sub_Status__c,
    &quot;Due By&quot;, 1,
    &quot;On Payment Plan&quot;, 1,
    &quot;Overdue&quot;, 1,
    &quot;Delinquent&quot;, 1,
    &quot;Credit&quot;, 1,
    0) == 1, TRUE, FALSE))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Has Invoiced Balance</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
