    <fields>
        <fullName>Five9_Call_Time_Mins__c</fullName>
        <externalId>false</externalId>
        <formula>/* Get total seconds for the Handle Time */

(
VALUE(RIGHT(Five9__Five9HandleTime__c,2)) + 
(VALUE(MID(Five9__Five9HandleTime__c, 4,2)) *60) + /* converts the minutes to seconds */
(VALUE(LEFT(Five9__Five9HandleTime__c,2)) *3600) /* converts the hours to seconds */
)
- /* Subtract total seconds for Wrap (after call) Time */

(
VALUE(RIGHT(Five9__Five9WrapTime__c,2)) + 
(VALUE(MID(Five9__Five9WrapTime__c, 4,2)) *60) + /* converts the minutes to seconds */
(VALUE(LEFT(Five9__Five9WrapTime__c,2)) *3600) /* converts the hours to seconds */
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Five9 Call Time (mins)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
