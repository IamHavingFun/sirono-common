    <fields>
        <fullName>Day_Of_Week__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
ISBLANK(ActivityDate), NULL,
CASE(
  MOD( ActivityDate  - DATE(1900, 1, 8), 7 ),
  0, &quot;1 Monday&quot;,
  1, &quot;2 Tuesday&quot;,
  2, &quot;3 Wednesday&quot;,
  3, &quot;4 Thursday&quot;,
  4, &quot;5 Friday&quot;,
  5, &quot;6 Saturday&quot;, 
  &quot;7 Sunday&quot;
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Day of Week</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
